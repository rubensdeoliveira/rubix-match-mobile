{
  "expo": {
    "name": "rubix-match-mobile",
    "slug": "rubix-match-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/application/assets/images/icon.png",
    "scheme": "rubixmatchmobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rubinhodev.rubixmatch",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./src/application/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.rubinhodev.rubixmatch",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/application/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/application/assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
				"expo-build-properties",
				{
					"ios": {
						"useFrameworks": "static"
					}
				}
			],
			"@react-native-firebase/app",
			"@react-native-firebase/auth"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "36701974-0b1a-4779-9e6e-83254dada806"
      }
    }
  }
}
